[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	di = diff
	dic = diff --cached
	pl = pull
	plp = pull --prune
	delete-merged-branches = !git branch --merged | grep -v \\* | grep -v master | grep -v develop | xargs -I % git branch -d %
[color]
	ui = auto
[core]
[push]
	default = simple
[pager]
	diff = diff-highlight | less
	show = diff-highlight | less
	log = diff-highlight | less
